{
    "name": "facturacion",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky install",
        "test": "rm -rf `find . -type d -name __snapshots__` && f() { if [ $npm_config_name ]; then jest --detectOpenHandles --coverage --testNamePattern=$npm_config_name; else jest --detectOpenHandles --coverage; fi; }; f",
        "test:one": "jest --coverage"
    },
    "dependencies": {
        "@nextui-org/react": "^1.0.0-beta.6",
        "bootstrap": "^5.1.3",
        "classnames": "^2.3.1",
        "next": "12.1.6",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-html-parser": "^2.0.2",
        "react-icomoon": "^2.5.3",
        "sass": "^1.49.9",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^12.1.6",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "12",
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.43",
        "@typescript-eslint/eslint-plugin": "^5.22.0",
        "eslint": "8.15.0",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.29.4",
        "husky": ">=7",
        "jest": "^28.1.0",
        "jest-environment-jsdom": "^28.1.0",
        "lint-staged": ">=10",
        "prettier": "^2.6.2",
        "typescript": "^4.6.3"
    },
    "engines": {
        "npm": "please-use-yarn",
        "yarn": ">= 1.19.1"
    },
    "lint-staged": {
        "*.{js,tsx}": "eslint --cache --fix",
        "*.{js,tsx,css,scss,md}": "prettier --write",
        "*.test.{tsx,ts}": "jest --detectOpenHandles --coverage --testNamePattern=$npm_config_name"
    }
}
